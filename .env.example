# Tauri Signing Key Configuration
# Copy this file to .env.local and update the values
# .env.local is ignored by git - never commit your actual keys!

# Path to your signing private key file
TAURI_SIGNING_PRIVATE_KEY_PATH=~/.tauri/opensw.key

# Password used when generating the key
TAURI_SIGNING_PRIVATE_KEY_PASSWORD=your-password-here
